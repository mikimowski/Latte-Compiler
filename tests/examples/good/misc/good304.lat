int main() {
    int size = 10;
    int i = 0;
    int[] arr = new int[size];
    while (i < size) {
        arr[i] = calculate(i);
        print(arr);
        i++;
    }
    return 0;
}

int print(int[] arr) {
    printString("");
    printString("Array");
    for (int i: arr) {
        calculate(i);
        int x = 5;
        string s = ":)";
        printInt(i);
        s = ":(";
        calculate(i);
    }
    return 42;
}

int calculate(int i) {
    int[] arr = new int[i+1];
    int j = 0;
    while (j <= i) {
        arr[j] = j;
        j++;
    }
    return arr[i];
}
